{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "next/core-web-vitals",
  "plugins": ["react", "react-hooks", "@typescript-eslint", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": [2, { "caseSensitive": false }],
    "react-hooks/exhaustive-deps": "off"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
