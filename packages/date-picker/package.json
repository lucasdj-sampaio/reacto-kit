    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.5.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
