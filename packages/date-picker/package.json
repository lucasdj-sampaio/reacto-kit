{
  "name": "@reacto-kit/date-picker",
  "version": "1.0.5",
  "private": false,
  "main": "dist/index-with-css.js",
  "module": "dist/index-with-css.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasdj-sampaio/reacto-kit/tree/main/packages/date-picker"
  },
  "homepage": "https://github.com/lucasdj-sampaio/reacto-kit/blob/main/packages/date-picker/readme.md",
  "exports": {
    ".": {
      "import": "./dist/index-with-css.mjs",
      "require": "./dist/index-with-css.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/styles.css",
    "dist/styles-inject.js",
    "dist/styles-inject.mjs",
    "dist/index-with-css.js",
    "dist/index-with-css.mjs"
  ],
  "keywords": [
    "React component with tailwind",
    "React date picker",
    "React ui components"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "test": "vitest",
    "build:css": "node ./scripts/build-tailwind.js",
